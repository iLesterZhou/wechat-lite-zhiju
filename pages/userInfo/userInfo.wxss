/* pages/userInfo/userInfo.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #eeeded;
}
.myDevices{
  width: 93%;
  height: 200rpx;
  margin: 10rpx auto;
  background-color: #fff;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  display: flex;
 
  
}
.userLogo{
  width: 20%;
}
.userLogo image{
  width:120rpx;
  height:120rpx;
  border-radius: 60rpx;
  margin:40rpx 20rpx;
}
.userInfo{
  margin-top: 30rpx;
  height: 40rpx;
  width: 60%;
  margin-left: 30rpx;
}
.userName{
  font-size:30rpx;
}
.devNum{
  font-size:20rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  color:rgb(128, 126, 126);
}
.userNum{
  font-size:20rpx;
  margin-top: 10rpx;
  color:rgb(128, 126, 126);
  display: flex;
  height: 30rpx;
  line-height: 30rpx;
}
.userNum image{
  width: 35rpx;
  height: 33rpx;
}
.userAut{
  background-color: mediumseagreen;
  color:#fff;
  width: 70rpx;
  height: 30rpx;
  font-size:18rpx;
  text-align: center;
  line-height: 30rpx;
  margin-left: 10rpx;
  border-radius: 8rpx;
}
.userJump{
  width: 120rpx;
  border-radius: 0 20rpx 20rpx 0 ;
  border-style: none;
  background-color: #fff;
}
.userHome{
  width: 10%;
  height: 60rpx;
  margin-top: 70rpx;
}

.userHome image{
  width: 60rpx;
  height: 60rpx;
}