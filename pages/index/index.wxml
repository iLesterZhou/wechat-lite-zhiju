<view class="container">
  <view>
  <text>微信智能小管家</text>
  </view>
  <view class='humdTemp-container'>
    <image src="{{value.Humdlogo}}" class='humd-logo'></image>
    <image src="{{value.Templogo}}" class='temp-logo'></image>
  </view>
  <view>
    <text class='humd-motto'>湿度：{{value.HumdValue}}%</text>
    <text>温度：{{value.TempValue}}°</text>
  </view>
  <view class='led-container'>
    <image src="{{LEDValue[0].LEDlogo}}" class='led-logo'></image>
  </view>

  <view>
    <button type="defaule" size="mini" bindtap="LedControl">{{LEDValue[0].ButtonValue}}</button>
  </view>
  <view>
    <image src='./../images/devil.png' class='logo'></image>
  </view>

</view>